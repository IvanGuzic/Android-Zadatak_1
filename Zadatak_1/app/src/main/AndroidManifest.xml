<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="guzic.recycleview">

    <uses-permission android:name="android.permission.INTERNET"/>

    <!-- izmjena na ''android:icon="@mipmap/new_icon"'' i ''android:roundIcon="@mipmap/new_icon_round"''-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/new_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/new_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RecycleView">
        <activity android:name="guzic.recycleview.DetaljiActivity"></activity>
        <activity android:name="guzic.recycleview.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>